<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/profile_admin_relative_layout"
        android:layout_width="match_parent"
        android:layout_height="100dp">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageView"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/man" />

        <TextView
            android:id="@+id/profile_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_10"
            android:layout_toEndOf="@id/imageView"
            android:text="Mr Naveen Yadav"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/profile_designation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/profile_name"
            android:layout_toEndOf="@id/imageView"
            android:text="Gis Software Head"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/profile_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/profile_designation"
            android:layout_toEndOf="@id/imageView"
            android:text="GMDA"
            android:textSize="12sp" />


        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/admin_switch_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp" />

        <TextView
            android:id="@+id/mode_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/admin_switch_btn"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="20sp"
            android:text="User Mode"
            android:textColor="@color/bgColor"
            android:textSize="10sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/admin_self_mode_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/profile_admin_relative_layout"
        android:orientation="vertical"
        android:visibility="gone">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginBottom="4dp"
            app:cardBackgroundColor="#f8f8f8">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="4dp"
                    app:cardMaxElevation="4dp"
                    app:cardUseCompatPadding="true">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <TextView
                            android:id="@+id/your_day_in_card_tv"
                            android:layout_width="match_parent"
                            android:layout_height="25sp"
                            android:background="@color/bgColor"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:text="@string/welcome_your_day_in_card"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <ImageView
                            android:id="@+id/img_selectfrom"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginLeft="30dp"
                            android:layout_marginTop="10dp"
                            android:src="@drawable/circle_app_color"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/your_day_in_card_tv" />


                        <View

                            android:id="@+id/view_view"
                            android:layout_width="0dp"
                            android:layout_height="2dp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginEnd="3dp"
                            android:background="@drawable/view_shape_line"
                            app:layout_constraintBottom_toBottomOf="@id/img_selectfrom"
                            app:layout_constraintLeft_toRightOf="@id/img_selectfrom"
                            app:layout_constraintRight_toLeftOf="@id/check_out_circle"
                            app:layout_constraintTop_toTopOf="@id/img_selectfrom" />

                        <ImageView
                            android:id="@+id/check_out_circle"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="30dp"
                            android:src="@drawable/circle_app_color"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/your_day_in_card_tv" />


                        <TextView

                            android:id="@+id/punch_in_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Punch In"
                            android:textColor="@color/bgColor"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            app:layout_constraintLeft_toLeftOf="@id/img_selectfrom"
                            app:layout_constraintRight_toRightOf="@id/img_selectfrom"
                            app:layout_constraintTop_toBottomOf="@id/img_selectfrom" />


                        <TextView
                            android:id="@+id/punch_out_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Punch Out"
                            android:textColor="@color/bgColor"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            app:layout_constraintLeft_toLeftOf="@id/check_out_circle"
                            app:layout_constraintRight_toRightOf="@id/check_out_circle"
                            app:layout_constraintTop_toBottomOf="@id/check_out_circle" />


                        <TextView

                            android:id="@+id/punch_in_time_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="-----"
                            android:textColor="@color/bgColor"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            app:layout_constraintLeft_toLeftOf="@id/img_selectfrom"
                            app:layout_constraintRight_toRightOf="@id/img_selectfrom"
                            app:layout_constraintTop_toBottomOf="@id/punch_in_text_view" />


                        <TextView

                            android:id="@+id/punch_out_time_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="------"
                            android:textColor="@color/bgColor"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            app:layout_constraintLeft_toLeftOf="@id/check_out_circle"
                            app:layout_constraintRight_toRightOf="@id/check_out_circle"
                            app:layout_constraintTop_toBottomOf="@id/punch_out_text_view" />


                        <TextView

                            android:id="@+id/working_hours_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Working Hours - -----"
                            android:textColor="@color/yellow"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@id/punch_out_time_tv"
                            app:layout_constraintLeft_toRightOf="@id/img_selectfrom"
                            app:layout_constraintRight_toLeftOf="@id/check_out_circle"
                            app:layout_constraintTop_toTopOf="@id/punch_in_time_tv" />


                        <Button
                            android:id="@+id/attendance_punch_in_day_btn"
                            android:layout_width="200dp"
                            android:layout_height="45dp"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/btn_background2"
                            android:text="@string/check_in_your_day"
                            android:textColor="@color/white"
                            android:textSize="12sp"
                            android:visibility="visible"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent" />

                        <Button
                            android:id="@+id/attendance_punch_out_day_btn"
                            android:layout_width="200dp"
                            android:layout_height="45dp"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/btn_background_red"
                            android:text="@string/check_out_your_day"
                            android:textColor="@color/white"
                            android:textSize="12sp"
                            android:visibility="gone"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent" />


                        <TextView
                            android:id="@+id/you_day_marked_successfully_tv"
                            android:layout_width="match_parent"
                            android:layout_height="25sp"
                            android:layout_marginBottom="20dp"
                            android:fontFamily="sans-serif"
                            android:gravity="center"
                            android:text="Your Day is Marked Successfully"
                            android:textAlignment="center"
                            android:textColor="@color/bgColor"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:visibility="gone"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:background="#f8f8f8"
                    android:orientation="vertical">


                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:gravity="center"
                        android:text="@string/your_current_month_status"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/attendance_view"
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:background="@color/bgColor" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/total_present"
                            android:textAlignment="center"
                            android:textColor="@color/green"
                            android:textSize="12sp"
                            android:textStyle="bold" />


                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/total_absent"
                            android:textAlignment="center"
                            android:textColor="@color/red"
                            android:textSize="12sp"
                            android:textStyle="bold" />


                        <TextView

                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/total_late"
                            android:textAlignment="center"
                            android:textColor="@color/yellow"
                            android:textSize="12sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layout_ticker_record"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="0dp"
                        android:orientation="horizontal"
                        android:weightSum="3">



                        <com.robinhood.ticker.TickerView
                            android:id="@+id/presentTicker_admin"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/line_background"
                            android:gravity="center"
                            android:shadowColor="@android:color/black"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"
                            android:text="0"
                            android:textColor="@color/green"
                            android:textSize="25sp"
                            android:textStyle="bold"
                            android:typeface="monospace"
                            app:ticker_animationDuration="1500"
                            app:ticker_defaultCharacterList="number" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/absentTicker_admin"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/line_background"
                            android:gravity="center"
                            android:shadowColor="@android:color/black"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"
                            android:text="0"
                            android:textColor="@color/red"
                            android:textSize="25sp"
                            android:textStyle="bold"
                            android:typeface="monospace"
                            app:ticker_animationDuration="1500"
                            app:ticker_defaultCharacterList="number" />

                        <com.robinhood.ticker.TickerView
                            android:id="@+id/late_ticker_admin"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/line_background"
                            android:gravity="center"
                            android:shadowColor="@android:color/black"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"
                            android:text="0"
                            android:textColor="@color/yellow"
                            android:textSize="25sp"
                            android:textStyle="bold"
                            android:typeface="monospace"
                            app:ticker_animationDuration="1500"
                            app:ticker_defaultCharacterList="number" />

                    </LinearLayout>


                </LinearLayout>


                <androidx.cardview.widget.CardView
                    android:id="@+id/report_card_view_click"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="3dp"
                    app:cardMaxElevation="3dp"
                    app:cardUseCompatPadding="true">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <ImageView
                            android:id="@+id/report_image_view"
                            android:layout_width="80dp"
                            android:layout_height="match_parent"
                            android:padding="6dp"
                            android:src="@drawable/attendance" />


                        <TextView
                            android:id="@+id/your_attendance_report"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_toRightOf="@id/report_image_view"
                            android:text="Your Attendance Report"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/your_attendance_report_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/your_attendance_report"
                            android:layout_toRightOf="@id/report_image_view"
                            android:text="Just One Tap &amp; get your all report"
                            android:textSize="10sp" />


                        <TextView
                            android:id="@+id/your_attendance_report_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="5dp"
                            android:layout_toRightOf="@id/report_image_view"
                            android:text="ðŸ“Š Present    ðŸ•’ Working Hours   ðŸ“‹ Status"
                            android:textColor="@color/bgColor"
                            android:textSize="10sp"
                            android:textStyle="bold" />


                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentEnd="true"
                            android:padding="10dp"
                            android:src="@drawable/arrow" />

                    </RelativeLayout>


                </androidx.cardview.widget.CardView>


            </LinearLayout>
        </androidx.cardview.widget.CardView>


    </LinearLayout>


    <LinearLayout
        android:id="@+id/admin_mode_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/profile_admin_relative_layout"
        android:orientation="vertical"
        android:visibility="visible">


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardCornerRadius="5dp"
            app:cardElevation="4dp"
            app:cardMaxElevation="4dp"
            app:cardUseCompatPadding="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#f8f8f8"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="center"
                    android:text="Your Team Today Status"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/attendance_view_2"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@color/bgColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal"
                    android:weightSum="4">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Total Employees:"
                        android:textAlignment="center"
                        android:textColor="@color/bgColor"
                        android:textSize="12sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/total_present"
                        android:textAlignment="center"
                        android:textColor="@color/green"
                        android:textSize="12sp"
                        android:textStyle="bold" />


                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/total_absent"
                        android:textAlignment="center"
                        android:textColor="@color/red"
                        android:textSize="12sp"
                        android:textStyle="bold" />


                    <TextView

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/total_late"
                        android:textAlignment="center"
                        android:textColor="@color/yellow"
                        android:textSize="12sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/team_ticker_layout"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:weightSum="4">


                    <com.robinhood.ticker.TickerView
                        android:id="@+id/total_staff"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/line_background"
                        android:gravity="center"
                        android:shadowColor="@android:color/black"
                        android:shadowDx="1"
                        android:shadowDy="1"
                        android:shadowRadius="2"
                        android:text="0"
                        android:textColor="@color/bgColor"
                        android:textSize="25sp"
                        android:textStyle="bold"
                        android:typeface="monospace"
                        app:ticker_animationDuration="1500"
                        app:ticker_defaultCharacterList="number" />
                    <com.robinhood.ticker.TickerView
                        android:id="@+id/team_present_tc"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/line_background"
                        android:gravity="center"
                        android:shadowColor="@android:color/black"
                        android:shadowDx="1"
                        android:shadowDy="1"
                        android:shadowRadius="2"
                        android:text="0"
                        android:textColor="@color/green"
                        android:textSize="25sp"
                        android:textStyle="bold"
                        android:typeface="monospace"
                        app:ticker_animationDuration="1500"
                        app:ticker_defaultCharacterList="number" />

                    <com.robinhood.ticker.TickerView
                        android:id="@+id/team_absent_ticker"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/line_background"
                        android:gravity="center"
                        android:shadowColor="@android:color/black"
                        android:shadowDx="1"
                        android:shadowDy="1"
                        android:shadowRadius="2"
                        android:text="0"
                        android:textColor="@color/red"
                        android:textSize="25sp"
                        android:textStyle="bold"
                        android:typeface="monospace"
                        app:ticker_animationDuration="1500"
                        app:ticker_defaultCharacterList="number" />

                    <com.robinhood.ticker.TickerView
                        android:id="@+id/team_late_ticker"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/line_background"
                        android:gravity="center"
                        android:shadowColor="@android:color/black"
                        android:shadowDx="1"
                        android:shadowDy="1"
                        android:shadowRadius="2"
                        android:text="0"
                        android:textColor="@color/yellow"
                        android:textSize="25sp"
                        android:textStyle="bold"
                        android:typeface="monospace"
                        app:ticker_animationDuration="1500"
                        app:ticker_defaultCharacterList="number" />

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Today Attendance Record : "
                        android:textColor="@color/bgColor"
                        android:textSize="11sp"
                        android:textStyle="bold" />


                    <TextView

                        android:id="@+id/see_all_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:text="@string/see_all"
                        android:textColor="@color/bgColor"
                        android:textSize="11sp"
                        android:textStyle="bold" />

                </RelativeLayout>


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/admin_recycler_view_dashboard"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />


                <androidx.cardview.widget.CardView
                    android:id="@+id/team_report_card_view_click"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="3dp"
                    app:cardMaxElevation="3dp"
                    app:cardUseCompatPadding="true">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <ImageView
                            android:id="@+id/team_report_image_view"
                            android:layout_width="80dp"
                            android:layout_height="match_parent"
                            android:padding="6dp"
                            android:src="@drawable/attendance" />


                        <TextView
                            android:id="@+id/team_attendance_report"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_toRightOf="@id/team_report_image_view"
                            android:text="Team Attendance Report"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="14sp"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/team_attendance_report_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
